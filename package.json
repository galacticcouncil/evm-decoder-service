{
  "name": "evm-log-decoder-service",
  "version": "1.0.0",
  "description": "Service for decoding EVM logs from a PostgreSQL database",
  "main": "evm-decoder-service.js",
  "scripts": {
    "start": "node evm-decoder-service.js",
    "docker:build": "docker build -t galacticcouncil/evm-log-decoder-service .",
    "docker:push": "docker push galacticcouncil/evm-log-decoder-service",
    "stack:deploy": "docker stack deploy -c docker-compose.yml evm-decoder"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "ethers": "^5.7.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "ethereum",
    "evm",
    "blockchain",
    "logs",
    "decoder",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
